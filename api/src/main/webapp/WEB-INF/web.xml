<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>SteamFeeds_servlet</display-name>
    <servlet-name>SteamFeeds_servlet</servlet-name>
    <servlet-class>com.newtonjoshua.SteamFeeds_servlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SteamFeeds_servlet</servlet-name>
    <url-pattern>/steamfeeds</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>Feeds_servlet</display-name>
    <servlet-name>Feeds_servlet</servlet-name>
    <servlet-class>com.newtonjoshua.Feeds_servlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Feeds_servlet</servlet-name>
    <url-pattern>/feed</url-pattern>
  </servlet-mapping>
</web-app>